{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Elasticsearch": {
    "Url": "http://localhost:9200",
    "DefaultIndex": "spareparts",
    "QueryFields": // s√∏gbare felter
    [
      "sparePartNo", "sparePartSerialCode", "name", "description", "typeNo", "notes", "currency",
      "manufacturerNo", "manufacturerName", "manufacturerNotes",
      "categoryNo", "categoryName", "categoryDescription",
      "supplierNo", "supplierName", "supplierContactInfo", "supplierNotes",
      "locationNo", "locationName", "locationArea", "locationBuilding", "locationNotes",
      "unitNo", "unitName"
    ]
  },
  "Cors": {
    "AllowAll": true
  }
}